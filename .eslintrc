{
  "plugins": ["react-hooks"],
  "extends": ["react-app", "plugin:prettier/recommended"],
  "rules": {
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "warn",
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "quotes": ["error", "double"]
    // "indent": ["error", 2, { "SwitchCase": 1 }]
  }
}